{  
  "name": "JAV-Magnet",
  "version": "0.1",
  "manifest_version": 3,
  
  "description": "JAV磁力搜索",
  "icons": {
      "16": "images/16.png",
      "48": "images/48.png",
      "128": "images/128.png"
   },
  "update_url": "http://clients2.google.com/service/update2/crx",

  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules_1.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "clipboardWrite",
    "clipboardRead"
  ],
  "host_permissions": [
    "*://*/*"
  ],

  "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls\u003E" ],
      "all_frames": true
   } ]
}